{
    "_comment": "SSHPot Command Configuration - Commands are ordered by customization priority",
    "_comment_structure": "Section 1: Highly Customizable (System Identity) | Section 2: Moderately Customizable (System State) | Section 3: Fixed Responses (Errors & Standard Commands)",

    "_section_1": "============================================================",
    "_section_1_title": "HIGHLY CUSTOMIZABLE - System Identity & Configuration",
    "_section_1_purpose": "Commands users will likely customize to match their honeypot environment",
    "_section_1_end": "============================================================",

    "hostname": {
        "output": "debian",
        "latency_ms": [30, 70],
        "customizable": true,
        "description": "System hostname - customize to match target environment"
    },

    "whoami": {
        "output": "root",
        "latency_ms": [50, 150],
        "customizable": true,
        "description": "Current user - typically 'root' but may vary by scenario"
    },

    "uname": {
        "output": "dynamic",
        "latency_ms": [40, 100],
        "customizable": true,
        "file_operations": {
            "type": "system_info",
            "requires_args": false,
            "flags": {
                "-a": {"all": true},
                "-r": {"kernel_release": true},
                "-s": {"kernel_name": true},
                "-m": {"machine": true},
                "-n": {"nodename": true}
            },
            "output_type": "system_info",
            "default_values": {
                "kernel_name": "Linux",
                "nodename": "debian",
                "kernel_release": "5.4.0-42-generic",
                "kernel_version": "#46-Ubuntu SMP Fri Jul 10 00:24:02 UTC 2020",
                "machine": "x86_64",
                "processor": "x86_64",
                "platform": "x86_64",
                "os": "GNU/Linux"
            }
        },
        "description": "System information - customize kernel version, architecture to match target"
    },

    "lscpu": {
        "output": "Architecture: x86_64\nCPU(s): 4\nModel name: Intel(R) Core(TM) i5-8250U CPU @ 1.60GHz\nThread(s) per core: 2\nCore(s) per socket: 2",
        "latency_ms": [60, 150],
        "customizable": true,
        "description": "CPU information - customize to match realistic hardware profile"
    },

    "free": {
        "output": "               total        used        free      shared  buff/cache   available\nMem:        8052532     {{random:3000000:3500000}}     {{random:2000000:2500000}}      {{random:100000:150000}}     {{random:2400000:2600000}}     {{random:5000000:5500000}}\nSwap:       2097148           0     2097148",
        "latency_ms": [50, 120],
        "customizable": true,
        "dynamic_values": true,
        "description": "Memory usage - uses randomized memory values for realistic variation"
    },

    "df": {
        "output": "Filesystem     1K-blocks    Used Available Use% Mounted on\n/dev/sda1       20971520 8388608  11534336  45% /\ntmpfs            4026266       0   4026266   0% /dev/shm",
        "latency_ms": [60, 150],
        "customizable": true,
        "description": "Disk space - customize partition sizes and usage percentages"
    },

    "uptime": {
        "output": "{{datetime:%H:%M:%S}} up {{uptime_days}}, 1 user, load average: {{random:0.00:0.15:2}}, {{random:0.01:0.20:2}}, {{random:0.05:0.30:2}}",
